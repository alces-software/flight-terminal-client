  "scripts": {
    "start": "PORT=4001 node scripts/customized-config start",
    "build": "node scripts/customized-config build && NODE_ENV=production webpack --config config/webpack.server.config.js",
    "build:client": "node scripts/customized-config build",
    "build:server": "NODE_ENV=production webpack --config config/webpack.server.config.js",
    "build:licensables": "license-checker --json --production --out src/data/licenses.json",
    "lint": "eslint src/**/*.js server/**/*.js; exit 0",
    "lint:watch": "esw -w src/**/*.js server/**/*.js",
    "test": "node scripts/customized-config test --env=jsdom",
    "eject": "react-scripts eject",
    "start:server": "APPROOT=$(pwd) NODE_ENV=development nodemon --watch src --watch server server/index.js",
    "serve": "APPROOT=$(pwd) node build/server/server.js"
  },
  "proxy": "http://localhost:4003"
